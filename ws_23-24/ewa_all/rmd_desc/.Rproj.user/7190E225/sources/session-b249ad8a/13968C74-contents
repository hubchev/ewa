         \AttributeTok{exclude\_fill\_values =} \FunctionTok{c}\NormalTok{(}\StringTok{"Weiß nicht"}\NormalTok{, }\StringTok{"Verweigert"}\NormalTok{),}
         \AttributeTok{sort =} \StringTok{"ascending"}
\NormalTok{         )}
\end{Highlighting}
\end{Shaded}

