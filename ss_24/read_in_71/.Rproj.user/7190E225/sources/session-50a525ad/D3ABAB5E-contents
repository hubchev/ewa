library(tidyverse)

# Example dataframe (replace with your actual dataframe)
df <- tibble(
  item_1 = c(1, NaN, 3, 2, 5),
  item_2 = c(NaN, 2, NaN, 4, 5),
  other_var = c(1, 2, 3, 4, 5)
)

# Count NaN values for each column that starts with "item_"
nan_counts <- df %>%
  summarise(across(starts_with("item_"), ~ sum(is.nan(.))))

# Print the counts
print(nan_counts)
